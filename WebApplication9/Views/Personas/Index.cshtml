@model List<WebApplication9.Models.Persona>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Persona</h1>

<p>
    <a asp-action="Crear">Crear Persona</a>
</p>


<table class="table">
    <thead>
        <tr>
            <th>
                Cedula
            </th>
            <th>
                Nombre
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Persona item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(a => item.Cedula)
                </td>
                <td>@item.Nombre</td>
            </tr>

        }
    </tbody>
</table>
